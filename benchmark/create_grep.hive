DROP TABLE grep;
DROP TABLE grep_select;

CREATE EXTERNAL TABLE grep ( key STRING, field STRING ) ROW FORMAT DELIMITED FIELDS TERMINATED BY '\t' STORED AS TEXTFILE LOCATION '/hive/input/grep';

CREATE TABLE grep_select ( key STRING, field STRING );

LOAD DATA INPATH '/input/hive/grep/part-00000' OVERWRITE INTO TABLE grep;
LOAD DATA INPATH '/input/hive/grep/part-00001' OVERWRITE INTO TABLE grep;
